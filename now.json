{
    "version": 1,
    "name": "Kordel",
    "alias": "Kordel",
    "builds": [
        {
            "src": "**.html",
            "use": "@now/static"
        },
        {
            "src": "/favicons/**",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "index.html"
        },
        {
            "src": "/favicons/(.*)",
            "dest": "/favicons/$1"
        }
    ]
}