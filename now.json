{
    "version": 1,
    "name": "Kordel",
    "alias": "kordel.now.sh",
    "builds": [
        {
            "src": "**.html",
            "use": "@now/static"
        },
        {
            "src": "**.js",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "index.html"
        }
    ]
}